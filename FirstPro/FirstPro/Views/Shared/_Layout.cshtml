<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <title>@ViewData["Title"] - FirstPro</title>
    <link rel="stylesheet" href="https://maxcdn.bootstrapcdn.com/bootstrap/3.4.1/css/bootstrap.min.css">
    <link rel="stylesheet" href="~/css/site.css" asp-append-version="true" />
    <link rel="stylesheet" href="~/FirstPro.styles.css" asp-append-version="true" />
</head>
<body>

    <header class="header">

        <div class="container">
            <div class="row">
        
                <div class="col-lg-6 col-md-6 col-sm-6 col-xl-6">
                    <a class="btn btn-primary" asp-action="AppplyNow" asp-controller="Home"> Apply Now </a>
                </div>

                <div class="col-lg-6 col-md-6 col-sm-6 col-xl-6 logo">
                    <h1> X Y Z </h1>
                </div>

            </div>
        </div>

    </header>

        <main role="main" class="pb-3">
            @RenderBody()
        </main>

    <script src="https://ajax.googleapis.com/ajax/libs/jquery/3.7.1/jquery.min.js"></script>
    <script src="https://maxcdn.bootstrapcdn.com/bootstrap/3.4.1/js/bootstrap.min.js"></script>
    <script src="~/js/site.js">  </script>
    
    @await RenderSectionAsync("Scripts", required: false)
</body>
</html>
